def qsort(a,l,h):
   def part(a,l,h):
      q = a[l]
      i = l
      j = h
      while i < j:
         while i < j and a[j] > q:
            j -= 1
          a[i], a[j] = a[j], a[i]
         while i< j and a[i] < = q:
            i +=1
          a[i], a[j] = a[j], a[i]
      return i
    
    if l < h:
       index = part(a,l,h)
       qsort(a,l, index-1)
       qsort(a,index+1,h)
       
       
